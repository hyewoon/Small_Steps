<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".tablLayout.MonthlyFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <SeekBar
        android:id="@+id/progress1"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:max="9000"
        android:progress="2026"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.329"
        android:rotation="270"
        android:translationX="-50dp"/>

    <SeekBar
        android:id="@+id/progress2"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="10"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        android:rotation="270"
        app:layout_constraintStart_toEndOf="@id/progress1"
        android:translationX="-200dp"
      />

    <SeekBar
        android:id="@+id/progress3"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="9000"
        android:progress="1120"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        app:layout_constraintStart_toEndOf="@id/progress2"
        android:rotation="270"
        android:translationX="-350dp"/>

    <SeekBar
        android:id="@+id/progress4"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="50"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        app:layout_constraintStart_toEndOf="@id/progress3"
        android:rotation="270"
        android:translationX="-500dp"/>
    <SeekBar
        android:id="@+id/progress5"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="50"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        app:layout_constraintStart_toEndOf="@id/progress4"
        android:rotation="270"
        android:translationX="-650dp"/>

    <SeekBar
        android:id="@+id/progress6"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="50"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        app:layout_constraintStart_toEndOf="@id/progress5"
        android:rotation="270"
        android:translationX="-800dp" />

    <SeekBar
        android:id="@+id/progress7"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:max="100"
        android:progress="50"
        android:progressDrawable="@drawable/custom_seekbar_progress"
        android:splitTrack="false"
        android:thumb="@drawable/custom_seekbar_thumb"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="211dp"
        app:layout_constraintTop_toTopOf="@id/progress1"
        app:layout_constraintBottom_toBottomOf="@id/progress1"
        app:layout_constraintStart_toEndOf="@id/progress6"
        android:rotation="270"
        android:translationX="-950dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>